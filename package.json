{
  "name": "playwright-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:headed": "npx playwright test --headed",
    "report": "npx playwright show-report",
    "lint": "eslint 'src/**/*.{ts,tsx}' --fix",
    "allure:generate": "npx allure generate allure-results --clean -o allure-report",
    "allure:serve": "npx allure serve allure-results",
    "allure:open": "npx allure open allure-report",
    "prettier": "prettier --write 'src/**/*.{ts,tsx,js,json,md}'",
    "clean:reports": "npx rimraf playwright-report allure-report allure-results test-results"
  },
